                $("#lblInactiveToday").text(data.InactiveToday);
                $("#lblBlockedToday").text(data.BlockedToday);
                $("#lblMainCatsToday").text(data.MainCatsToday);
                $("#lblSubCatsToday").text(data.SubCatsToday);
                $("#lblChildCatsToday").text(data.ChildCatsToday);
                $("#lblItemCatsToday").text(data.ItemCatsToday);
                $("#lblCitiesToday").text(data.CitiesToday);
                $("#lblStatesToday").text(data.StatesToday);
                $("#lblEnquiriesToday").text(data.EnquiriesToday);

                $("#progress").hide();
            },

            error: function () {
                $("#progress").hide();
                swal("Error loading data! Please try again.");
            }
        });
    });
</script>

