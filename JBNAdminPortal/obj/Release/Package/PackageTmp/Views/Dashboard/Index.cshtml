<div id="progress" class="modal">
    <div class="center" style="margin: 0px; padding: 0px; position: fixed; right: 0px; top: 0px; width: 100%; height: 100%; background-color: rgb(102, 102, 102); z-index: 30001; opacity: 0.8;">
        <p style="position: absolute; color: White; top: 45%; left: 45%;">
            <span>Loading, Please wait...</span> <img src="~/images/loading.gif" height="100" width="100" />
        </p>
    </div>
</div>

<script>
    $(document).ready(function () {
        $("#progress").show();
        $.ajax({
            type: 'POST',
            dataType: 'json',
            contentType: 'application/json',
            url: '@Url.Action("GetDashboardData", "Dashboard")',
            success: function (data) {
                $("#lblRegisteredCustomers").text(data.RegisteredCustomers);
                $("#lblActiveCustomers").text(data.ActiveCustomers);
                $("#lblInactiveCustomers").text(data.InactiveCustomers);
                $("#lblBlockedCustomers").text(data.BlockedCustomers);
                $("#lblMainCategories").text(data.MainCategories);
                $("#lblSubCategories").text(data.SubCategories);
                $("#lblChildCategories").text(data.ChildCategories);
                $("#lblItemCategories").text(data.ItemCategories);
                $("#lblCitiesCovered").text(data.CitiesCovered);
                $("#lblStatesCovered").text(data.StatesCovered);
