                                    <span>@Model.ActivePer <i class="ti-arrow-up"></i> @Model.CActiveUsersStatus</span>
                                }
                                else
                                {
                                    <span>@Model.ActivePer</span>
                                }

                            }
                        }
                    </td>
                </tr>
                <tr>
                    <td>
                        Inactive Users
                    </td>
                    <td>
                        @Model.InactiveUsers
                    </td>
                    <td>
                        @Model.CInactiveUsers
                    </td>
                    <td>
                        @{
                            if (Model.InactivePer < 0)
                            {
                                <span>@Model.InactivePer <i class="ti-arrow-down"></i></span>
                            }
