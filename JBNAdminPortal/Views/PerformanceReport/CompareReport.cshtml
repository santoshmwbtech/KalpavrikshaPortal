@model JBNClassLibrary.PerformanceReport

<div class="table-responsive pt-3">
    <table class="table table-bordered dataGrid">
        <thead>
            <tr>
                <th>

                </th>
                <th>
                    @Model.FromDate <span>To</span> @Model.ToDate
                </th>
                <th>
                    @Model.CompareFromDate <span>To</span> @Model.CompareToDate
                </th>
                <th>
                    Status
                </th>
            </tr>
        </thead>
        <tbody>
            @if (Model != null)
            {
                <tr>
                    <td>
                        Registered Users
                    </td>
                    <td>
                        @Model.RegisteredUser
                    </td>
                    <td>
                        @Model.CRegisteredUser
                    </td>
                    <td>
                        @{
                            if (Model.RegisteredPer < 0)
                            {
                                <span>@Model.RegisteredPer <i class="ti-arrow-down"></i></span>
                            }
                            else
                            {
                                if (Model.RegisteredPer > 0)
                                {
                                    <span>@Model.RegisteredPer <i class="ti-arrow-up"></i> @Model.CRegisteredStatus</span>
                                }
                                else
                                {
                                    <span>@Model.RegisteredPer</span>
                                }
                            }
                        }
                    </td>
                </tr>
                <tr>
                    <td>
                        Active Users
                    </td>
                    <td>
                        @Model.ActiveUsers
                    </td>
                    <td>
                        @Model.CActiveUsers
                    </td>
                    <td>
                        @{
                            if (Model.ActivePer < 0)
                            {
                                <span>@Model.ActivePer <i class="ti-arrow-down"></i></span>
                            }
                            else
                            {
                                if (Model.ActivePer > 0)
