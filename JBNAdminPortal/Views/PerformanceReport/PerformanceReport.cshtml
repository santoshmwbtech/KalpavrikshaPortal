@model JBNClassLibrary.PerformanceReport

<div class="table-responsive pt-3">
    <table class="table table-bordered dataGrid">
        <thead>
            <tr>
                <th>

                </th>
                <th>
                    @Model.FromDate <span>To</span> @Model.ToDate
                </th>
            </tr>
        </thead>
        <tbody>
            @if (Model != null)
            {
                <tr>
                    <td>
                        Registered Users
                    </td>
                    <td>
                        @Model.RegisteredUser
                    </td>
                </tr>
                <tr>
                    <td>
                        Active Users
                    </td>
                    <td>
                        @Model.ActiveUsers
                    </td>
                </tr>
                <tr>
                    <td>
                        Inactive Users
                    </td>
                    <td>
                        @Model.InactiveUsers
                    </td>
                </tr>
                <tr>
                    <td>
                        Blocked Users
                    </td>
                    <td>
                        @Model.BlockedUsers
                    </td>
                </tr>
                <tr>
                    <td>
                        Enquiries
                    </td>
                    <td>
                        @Model.TotalEnquiries
                    </td>
                </tr>
                <tr>
                    <td>
                        Main Categories
                    </td>
                    <td>
                        @Model.MainCategories
                    </td>
                </tr>
                <tr>
                    <td>
                        Sub Categories
                    </td>
                    <td>
                        @Model.SubCategories
                    </td>
                </tr>
                <tr>
                    <td>
                        Child Categories
                    </td>
                    <td>
                        @Model.ChildCategories
                    </td>
                </tr>
                <tr>
                    <td>
                        Item Categories
                    </td>
                    <td>
                        @Model.ItemCategories
                    </td>
                </tr>
                <tr>
                    <td>
                        Total States
                    </td>
                    <td>
                        @Model.TotalStates
                    </td>
                </tr>
                <tr>
                    <td>
                        Total Cities
                    </td>
                    <td>
                        @Model.TotalCities
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>
