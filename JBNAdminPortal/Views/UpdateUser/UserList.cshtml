@model IEnumerable<JBNClassLibrary.CustomerIncompleteRpt>

<div class="pt-3">
    <table class="table table-bordered dataGrid">
        <thead>
            <tr>
                <th>
                    Firm Name
                </th>
                <th>
                    Owner Name
                </th>
                <th>
                    City
                </th>
                <th>
                    State
                </th>
                <th>
                    Action
                </th>
            </tr>
        </thead>
        <tbody>
            @if (Model != null)
            {
                if (Model.Count() > 0)
                {
                    foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @Html.HiddenFor(Model => item.CustID)
                                @item.FirmName
                            </td>
                            <td>
                                @item.OwnerName
                            </td>
                            <td>
                                @item.CityName
                            </td>
                            <td>
                                @item.StateName
                            </td>
                            <td>
                                @Ajax.ActionLink("Edit", "Edit", "UpdateUser", new { CustID = item.CustID }, new AjaxOptions { UpdateTargetId = "divCreate", HttpMethod = "get", OnSuccess = "onEditSuccess", LoadingElementId = "progress" }, new { @class = "btn btn-danger btn-xs" })
                            </td>
                        </tr>
                    }
                }
            }
        </tbody>
    </table>
</div>
